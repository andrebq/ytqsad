{
  "description": "Speedup YT Adds",
  "manifest_version": 2,
  "name": "YouTube QuickSilver Ads",
  "version": "1.0",
  "homepage_url": "https://github.com/andrebq/ytqsad",
  "icons": {
    "48": "icons/border-48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*"
      ],
      "js": [
        "fast-ad.js"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "addon@example.com",
      "strict_min_version": "58.0"
    }
  }
}